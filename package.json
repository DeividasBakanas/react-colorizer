{
  "name": "react-colorizer",
  "version": "0.1.3",
  "description": "Library for generation of harmonious color themes.",
  "keywords": [
    "react",
    "colorizer",
    "color",
    "harmony",
    "generator"
  ],
  "author": {
    "name": "Rostyslav Khanas",
    "url": "https://github.com/rtkhanas"
  },
  "scripts": {
    "storybook-web-build": "cd ./storybooks/web && npm run build-storybook",
    "storybook-web": "cd ./storybooks/web && npm run storybook",
    "storybook-native-watch": "babel ./src --out-dir=./storybooks/native/lib --watch",
    "storybook-native": "cd ./storybooks/native && npm run storybook",
    "storybook-emulator-ios": "cd ./storybooks/native && react-native run-ios",
    "clean": "rimraf lib",
    "lint": "eslint src",
    "build": "npm run lint && npm run clean && babel src --out-dir lib",
    "release": "npm run build && npm run storybook-web-build"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:opensource-cards/react-colorizer.git"
  },
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "dependencies": {
    "tinycolor2": "^1.4.1"
  },
  "peerDependencies": {
    "react": "~15.3.2",
    "react-native": "^0.39.0",
    "styled-components": "^1.1.2"
  },
  "devDependencies": {
    "babel": "^6.3.26",
    "babel-cli": "^6.16.0",
    "babel-core": "^6.16.0",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-object-rest-spread": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "eslint": "^3.10.0",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.2",
    "eslint-plugin-react": "^6.6.0",
    "react": "~15.3.2",
    "react-dom": "~15.3.2",
    "react-native": "^0.39.0",
    "rimraf": "^2.5.4",
    "styled-components": "^1.1.2"
  }
}
